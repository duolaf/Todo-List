{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],

"rules" : {
    "object-literal-sort-keys": false,
    "no-console" : ["error", { "t": ["warn", "error"] }],
    "variable-name" : ["allow-leading-underscore"] ,
    "no-empty" : false,
    "unused" : false
  },

  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
